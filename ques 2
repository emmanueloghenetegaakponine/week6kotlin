fun love(flower1 : Int,flower2 : Int): Boolean{
    if ((flower1%2==0)&&(flower2%2!==0)) {
        return true;
    }else if((flower1%2!==0)&&(flower2%2==0)){
        return true;
    }else return false;
}

fun main(){
    println("${love(4,4)}")
    println("${love(3,11)}")
    println("${love(22,17)}")

}
